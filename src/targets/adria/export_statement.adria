
var Node = require('./base/node');
var Module = require('./module');

module ExportStatement = proto (Node) {

    toSourceNode: func() {

        var name = this.get('name').value;
        var moduleNode = this.findProto(Module);
        moduleNode.addExport(name);

        return this.csn([ name, ' = ', this.get('value').toSourceNode(), ';' ]);
    },
};